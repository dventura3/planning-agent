@prefix vocab: <http://www.example.org/vocab#>.
@prefix hydra: <http://www.w3.org/ns/hydra/core#>.
@prefix st: <http://www.mystates.org/states#>.



</plants/> a vocab:PlantsCollection.

</plants/1/> a vocab:Plant;
			vocab:hasAssociatedSensors [
					a vocab:SensorsPlantCollection;
					hydra:member </sensors/1/>
			];
			vocab:hasAssociatedActuators [
					a vocab:ActuatorsPlantCollection;
					hydra:member </actuators/1/>
			].


</plants/2/> a vocab:Plant;
			vocab:hasAssociatedSensors [
					a vocab:SensorsPlantCollection;
					hydra:member </sensors/2/>, </sensors/3/>
			];
			vocab:hasAssociatedActuators [
					a vocab:ActuatorsPlantCollection;
					hydra:member </actuators/2/>
			].


</sensors/> a vocab:SensorsCollection;
					hydra:member </sensors/1/>, </sensors/2/>, </sensors/3/>.

</sensors/1/> a vocab:TemperatureSensor.
</sensors/2/> a vocab:MoistureSensor.
</sensors/3/> a vocab:LightSensor.


</actuators/> a vocab:ActuatorsCollection;
					hydra:member </actuators/1/>, </actuators/2/>.

</actuators/1/> a vocab:IrrigationPump.
</actuators/2/> a vocab:IrrigationPump.

{ 
</actuators/1/> vocab:hasSwitchingState _:state.
</actuators/2/> vocab:hasSwitchingState _:state.
} a st:State.

